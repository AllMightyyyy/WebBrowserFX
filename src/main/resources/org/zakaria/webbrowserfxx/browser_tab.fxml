<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.web.WebView?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns:fx="http://javafx.com/fxml" fx:controller="org.zakaria.webbrowserfxx.controller.BrowserTabController">
    <!-- Top Toolbar with Navigation Buttons -->
    <top>
        <fx:include source="toolbar.fxml" fx:id="toolbar"/>
    </top>

    <!-- Center WebView -->
    <center>
        <WebView fx:id="webView" />
    </center>
</BorderPane>
